# PostgreSQL with pgvector (default for docker-compose)
DATABASE_URL=postgres://neural-brain:neural-brain@localhost:5433/neural-brain?sslmode=disable

# Path to GTE-Small model file (run scripts/setup-model.sh once)
GTE_MODEL_PATH=./models/gte-small.gtemodel

# HTTP server port
PORT=9124

# If set, skip inserting seeds that have cosine similarity > this with any existing seed (0 = disabled)
DEDUP_THRESHOLD=0.92
